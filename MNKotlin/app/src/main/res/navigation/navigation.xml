<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/loginFragment2">

    <fragment
        android:id="@+id/selectPictureFragment"
        android:name="org.first.mnkotlin.SelectPictureFragment"
        android:label="SelectPictureFragment"
        tools:layout="@layout/fragment_select_picture">
        <action
            android:id="@+id/action_selectPictureFragment_to_loginFragment2"
            app:destination="@id/loginFragment2" />
        <action
            android:id="@+id/action_selectPictureFragment_to_glideViewPagerActivity"
            app:destination="@id/glideViewPagerActivity" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment2"
        android:name="org.first.mnkotlin.LoginFragment"
        android:label="LoginFragment"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_loginFragment2_to_selectPictureFragment"
            app:destination="@id/selectPictureFragment"
            app:popUpTo="@id/loginFragment2"
            app:popUpToInclusive="true"/>

    </fragment>
    <fragment
        android:id="@+id/food1Framgent"
        android:name="org.first.mnkotlin.foodFragemnts.Food1Framgent"
        android:label="fragment_food1_framgent"
        tools:layout="@layout/fragment_food1" />
    <activity
        android:id="@+id/glideViewPagerActivity"
        android:name="org.first.mnkotlin.GlideViewPagerActivity"
        android:label="activity_glide_view_pager"
        tools:layout="@layout/activity_glide_view_pager" />
</navigation>